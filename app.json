{
  "name": "levanter-md@1.0.0",
  "description": "Hi there, I am Levanter-MD Whatsapp Bot built in NodeJs and Baileys to make experience better.\n\t Incase of Any Problem Whatsapp me on: +254728782591",
  "logo": "https://telegra.ph/file/54efddccf41281ad7ec51.jpg",
  "keywords": ["whatsappbot", "levanterMd", "levanter Md bot", "Levanter-Md", "LevanterMdWhatsaooBot", "LevanterWaBot","LevanterMD","Levanter Md Whatsapp Bot","multi device"],
   "success_url": "/levanter-md/",
  "app_url": "/qr/",
  
  "env": {
    "ALWAYS_ONLINE": {
      "description": "Show bot as always online",
      "required": true,
      "value": "true"
    },
    "LOG_MSG": {
      "description": "Show msgs in log",
      "required": true,
      "value": "false"
    },
    "SUDO": {
      "description": "admin",
      "required": true,
      "value": "admin"
    },
    "PREFIX": {
      "description": "prefix, check for more",
      "required": true,
      "value": ","
    },
    "SESSION_ID": {
      "description": "Put your SESSION_ID here.",
      "required": true,
      "value": "12_8_2ffd_0e98_9b04"
    },
    "STICKER_PACKNAME": {
      "description": "sticker pack info",
      "required": true,
      "value": "GiftedTech"
    },
    "HEROKU_APP_NAME": {
      "description": "Heroku app name, same as above entered.",
      "required": true
    },
    "HEROKU_API_KEY": {
      "description": "Heroku account api key, https://dashboard.heroku.com/account",
      "required": true,
      "value": "6e3c88bf-1cdd-4016-8641-96ff2aca4c73"
    },
    "RMBG_KEY": {
      "description": "API key from remove.bg, required for plugin rmbg",
      "required": true,
      "value": "DJwc34NaWmhXmDxEeh6YUEmm"
    },
    "LANGUAG": {
      "description": "Default Language for tts or trt",
      "required": true,
      "value": "en"
    },
    "WARN_LIMIT": {
      "description": "Maximum number of wanings to a person.",
      "required": true,
      "value": "5"
    },
    "FORCE_LOGOUT": {
      "description": "In Some scenario have to logout web, Only such case make this true and back to false after logout.",
      "required": true,
      "value": "false"
    },
    "ANTILINK_MSG": {
      "description": "Message send when antilink and action is kick.",
      "required": true,
      "value": "_Antilink Detected &mention kicked_"
    },
    "ANTISPAM_MSG": {
      "description": "Message send when antispam.",
      "required": true,
      "value": "_Antispam Detected &mention kicked_"
    },
    "ANTIWORDS_MSG": {
      "description": "Message send when antiword.",
      "required": true,
      "value": "_Antiword Detected &mention kicked_"
    },
    "ANTIWORDS": {
      "description": "Bad words that not allowed in chats.",
      "required": true,
      "value": "fuck,stupid,word3"
    },
    "SS_TOKEN": {
      "description": "token from https://app.screenshotapi.net/dashboard",
      "required": true,
      "value": "null"
    },
    "REJECT_CALL": {
      "description": "auto reject call, if set as true",
      "required": true,
      "value": "false"
    },
    "AUTO_STATUS_VIEW": {
      "description": "view others status",
      "required": true,
      "value": "true"
    },
    "SEND_READ": {
      "description": "Auto blue tick messages",
      "required": true,
      "value": "false"
    },
    "AJOIN": {
      "description": "Accept Group join request(Group privacy)",
      "required": true,
      "value": "true"
    },
    "PERSONAL_MESSAGE": {
      "description": "Send a Welcome message on personal chat",
      "required": true,
      "value": "null"
    },
    "DISABLE_START_MESSAGE": {
      "description": "Disable start Message",
      "required": true,
      "value": "false"
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql:basic"
    }
  ],

  "buildpacks": [{ "url": "https://github.com/carlosdommor/heroku-buildpack-ffmpeg-latest" }],
  "buildpacks": [{ "url": "https://github.com/heroku/heroku-buildpack-nodejs#latest" }],
  "formation": {
    "web": {
      "quantity": 1,
      "size": "standard-2x"
    }
  }
}
